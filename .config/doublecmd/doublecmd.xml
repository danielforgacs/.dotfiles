<?xml version="1.0" encoding="UTF-8"?>
<doublecmd DCVersion="0.9.8 beta" ConfigVersion="10">
  <Configuration Save="False" />
  <ColumnsSets>
    <ColumnsSet>
      <Name>Default</Name>
      <Columns>
        <Column>
          <Title>Name</Title>
          <FuncString>[DC().GETFILENAMENOEXT{}]</FuncString>
          <Width>500</Width>
        </Column>
        <Column>
          <Title>Ext</Title>
          <FuncString>[DC().GETFILEEXT{}]</FuncString>
          <Width>100</Width>
        </Column>
        <Column>
          <Title>Size</Title>
          <FuncString>[DC().GETFILESIZE{}]</FuncString>
          <Width>105</Width>
          <Align>1</Align>
        </Column>
        <Column>
          <Title>Date</Title>
          <FuncString>[DC().GETFILETIME{}]</FuncString>
          <Width>195</Width>
          <Align>1</Align>
        </Column>
      </Columns>
    </ColumnsSet>
  </ColumnsSets>
  <Behaviours>
    <Mouse>
      <Selection Enabled="True">
        <Button>1</Button>
      </Selection>
    </Mouse>
    <DriveBlackListUnmounted>True</DriveBlackListUnmounted>
  </Behaviours>
  <Miscellaneous>
    <DirBrackets>False</DirBrackets>
  </Miscellaneous>
  <Fonts>
    <Main>
      <Name>Monospace</Name>
      <Size>13</Size>
      <Style>0</Style>
      <Quality>0</Quality>
    </Main>
  </Fonts>
  <DirectoryHotList>
    <HotDir Name="storage" Path="/home/ford/storage/"/>
    <HotDir Name="media" Path="/media/ford/"/>
  </DirectoryHotList>
  <Tools>
    <Editor Enabled="True">
      <Path>code</Path>
    </Editor>
  </Tools>
  <Tabs>
    <OpenedTabs>
      <Left>
        <Tab>
          <FileView Type="columns">
            <History>
              <Entry Active="True">
                <FileSource Type="FileSystem"/>
                <Paths>
                  <Path Filename="..">/home/ford/storage/</Path>
                </Paths>
              </Entry>
            </History>
          </FileView>
        </Tab>
      </Left>
      <Right>
        <Tab>
          <FileView Type="columns">
            <History>
              <Entry Active="True">
                <FileSource Type="FileSystem"/>
                <Paths>
                  <Path Filename="..">/home/ford/storage/</Path>
                </Paths>
              </Entry>
            </History>
          </FileView>
        </Tab>
      </Right>
    </OpenedTabs>
  </Tabs>
  <Layout>
    <DriveBarFlat>False</DriveBarFlat>
    <DriveIndicator>True</DriveIndicator>
  </Layout>
  <FileOperations>
    <RenameSelOnlyName>True</RenameSelOnlyName>
    <TypeOfDuplicatedRename>2</TypeOfDuplicatedRename>
  </FileOperations>
  <AutoRefresh>
    <Options>7</Options>
  </AutoRefresh>
  <Colors>
    <UseInvertedSelection>True</UseInvertedSelection>
  </Colors>
</doublecmd>
